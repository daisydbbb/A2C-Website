  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "express": "^5.1.0",
    "mongoose": "^8.15.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "nodemon": "^3.1.10"
  }
